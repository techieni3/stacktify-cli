{
    "name": "techieni3/stacktify-cli",
    "description": "Create laravel application with your preferred stack.",
    "keywords": [
        "laravel",
        "starter-kit",
        "cli",
        "stacktify",
        "saas"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nitin Gaikwad",
            "email": "niteen1593@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/filesystem": "^12.32",
        "illuminate/support": "^12.32",
        "laravel/prompts": "^0.3.6",
        "symfony/console": "^7.3",
        "symfony/process": "^7.3"
    },
    "bin": [
        "bin/stacktify"
    ],
    "autoload": {
        "psr-4": {
            "Techieni3\\StacktifyCli\\": "src/"
        }
    },
    "scripts": {
        "format": [
            "rector",
            "pint --parallel"
        ],
        "test:types": "phpstan analyse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "dev-main"
    }
}
